<html lang="en">

<%- include('../partials/head') %>


<body>


    <section id="header">
        <h1>JBLM Unlimited</h1>
        <a class="home3" href="/">Home</a>
        <a class="calendar3" href="/calendar">Calendar</a>
      </section>
  <main>
    <h4>Resources</h4>
    <ul>

      <% resource.forEach(partner => { %>
      <li>
        <img src="<%= partner.logo_png %>">
        <h5>Title:<%= partner.title %></h5>
        <p><%= partner.description %></p>
        <a href="<%= partner.resource_url %>"><%= partner.resource_url %></a>
        <a href="/pdf">PDF</a>
      </li>
      <% }) %>

    </ul>
  </main>




    <div id="copyright"> &copy; 2019 JBLM Unlimited</div>
    
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
  <script src="/js/app.js"></script>
  <script>
    $('.resources_link').hide();
  </script>

</body>

</html>